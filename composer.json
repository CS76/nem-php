{
    "name": "evias/php-nem-laravel",
    "description": "Composer Package for PHP (Laravel), Wrapper to use NEM blockchain REST API",
    "keywords": ["laravel", "framework", "nem", "cryptocurrency", "api", "restful", "laravel-nem", "evias", "Gregory Saive"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gregory Saive",
            "email": "greg@evias.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "mashape/unirest-php": "3.*",
        "guzzlehttp/guzzle": "6.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "evias\\NEMBlockchain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "evias\\NEMBlockchain\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}